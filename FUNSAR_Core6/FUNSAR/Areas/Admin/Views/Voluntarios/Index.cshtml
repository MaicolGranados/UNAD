@using FUNSAR.Utilidades
@using FUNSAR.HerramientasComunes
@{
    ViewData["Title"] = "Participantes";
}
@try
{
    <div class="container mt-4">
        <div class="row">
            <div class="col-8">
                <h2 class="text-primary">Participantes Inscritos</h2>
            </div>
        </div>

        <br />
        <br />

        <table id="tblVoluntarios" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Nombre</th>
                    <th>Apellido</th>
                    <th>Tipo</th>
                    <th>Documento</th>
                    <th>Colegio</th>
                    <th>Proceso</th>
                    <th>Estado</th>
                    <th>Acciones</th>
                </tr>
            </thead>
        </table>
    </div>
    @if (User.IsInRole(CNT.Gestor))
    {
        @section Scripts{
    <script src="~/js/voluntariosGestor.js"></script>
}
    }
    @if (User.IsInRole(CNT.Administrativo) || User.IsInRole(CNT.SuperAdmin))
    {
        @section Scripts{
    <script src="~/js/voluntarios.js"></script>
}
    }
    @if (User.IsInRole(CNT.Division))
    {
        @section Scripts{
    <script src="~/js/voluntariosDivision.js"></script>
}
    }
}
catch (Exception ex)
{
    Log log = new Log();
    await log.escribirLog("ERROR", ex.ToString(), "RegistroParticimante.cshtml");
}

