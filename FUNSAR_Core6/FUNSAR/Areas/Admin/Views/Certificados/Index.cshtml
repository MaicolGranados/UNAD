@{
    ViewData["Title"] = "Certificado";
}
<div class="container mt-4">
    <div class="row">
        <div class="col-8">
            <h2 class="text-primary">Certificados Creados</h2>
        </div>
        <div class="col-4 text-bg-danger-rigth">
            <a asp-action="Masiva" class="btn btn-primary"><i class="fas fa-plus"> Carga Masiva</i></a>
            <a asp-action="Create" class="btn btn-primary mt-3"><i class="fas fa-plus"> Crear Nuevo Certificado</i></a>
        </div>
    </div>

    <br />
    <br />

    <table id="tblCertificados" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Apellido</th>
                <th>Tipo</th>
                <th>Documento</th>
                <th>Brigada</th>
                <th>Año Proceso</th>
                <th>Semestre</th>
                <th>Expedicion</th>
                <th>Acciones</th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{
    <script src="~/js/certificados.js"></script>
}